{
  "manifest_version": 3,
  "minimum_chrome_version": "88",
  "version": "8.0",
  "name": "Firebase MV3 Test",
  "description": "Testing Firebase with MV3",
  "host_permissions":[
    "<all_urls>"
  ],
  "background": {
  	"service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["test.js"],
      "run_at": "document_end"
    }
  ]
}
